<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> updateDisplay() {</p>
<p class="p4"><span class="Apple-converted-space">      </span>document.getElementById(<span class="s3">'display'</span>).innerText = current;</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> appendNumber(num) {</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">if</span> (current === <span class="s3">'0'</span> &amp;&amp; num !== <span class="s3">'.'</span>) {</p>
<p class="p4"><span class="Apple-converted-space">        </span>current = num;</p>
<p class="p4"><span class="Apple-converted-space">      </span>} <span class="s1">else</span> <span class="s1">if</span> (num === <span class="s3">'.'</span> &amp;&amp; current.includes(<span class="s3">'.'</span>)) {</p>
<p class="p4"><span class="Apple-converted-space">        </span><span class="s1">return</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>} <span class="s1">else</span> {</p>
<p class="p4"><span class="Apple-converted-space">        </span>current += num;</p>
<p class="p4"><span class="Apple-converted-space">      </span>}</p>
<p class="p4"><span class="Apple-converted-space">      </span>updateDisplay();</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> clearDisplay() {</p>
<p class="p4"><span class="Apple-converted-space">      </span>current = <span class="s3">'0'</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>previous = <span class="s3">''</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>operator = <span class="s1">null</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>updateDisplay();</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> toggleSign() {</p>
<p class="p4"><span class="Apple-converted-space">      </span>current = (parseFloat(current) * -<span class="s5">1</span>).toString();</p>
<p class="p4"><span class="Apple-converted-space">      </span>updateDisplay();</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> percentage() {</p>
<p class="p4"><span class="Apple-converted-space">      </span>current = (parseFloat(current) / <span class="s5">100</span>).toString();</p>
<p class="p4"><span class="Apple-converted-space">      </span>updateDisplay();</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> operate(op) {</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">if</span> (operator &amp;&amp; previous) {</p>
<p class="p4"><span class="Apple-converted-space">        </span>calculate();</p>
<p class="p4"><span class="Apple-converted-space">      </span>}</p>
<p class="p4"><span class="Apple-converted-space">      </span>operator = op;</p>
<p class="p4"><span class="Apple-converted-space">      </span>previous = current;</p>
<p class="p4"><span class="Apple-converted-space">      </span>current = <span class="s3">'0'</span>;</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> calculate() {</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">if</span> (!operator || !previous) <span class="s1">return</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">let</span> result;</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">const</span> prev = parseFloat(previous);</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">const</span> curr = parseFloat(current);</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">switch</span> (operator) {</p>
<p class="p4"><span class="Apple-converted-space">        </span><span class="s1">case</span> <span class="s3">'+'</span>: result = prev + curr; <span class="s1">break</span>;</p>
<p class="p4"><span class="Apple-converted-space">        </span><span class="s1">case</span> <span class="s3">'-'</span>: result = prev - curr; <span class="s1">break</span>;</p>
<p class="p4"><span class="Apple-converted-space">        </span><span class="s1">case</span> <span class="s3">'*'</span>: result = prev * curr; <span class="s1">break</span>;</p>
<p class="p4"><span class="Apple-converted-space">        </span><span class="s1">case</span> <span class="s3">'/'</span>: result = prev / curr; <span class="s1">break</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>}</p>
<p class="p4"><span class="Apple-converted-space">      </span>current = result.toString();</p>
<p class="p4"><span class="Apple-converted-space">      </span>operator = <span class="s1">null</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>previous = <span class="s3">''</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>updateDisplay();</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> showDigitSum() {</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">const</span> result = current;</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">const</span> digits = result.replace(<span class="s7">/[^0-9]/g</span>, <span class="s3">''</span>);</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">let</span> sum = <span class="s5">0</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">for</span> (<span class="s1">let</span> digit <span class="s1">of</span> digits) sum += parseInt(digit);</p>
<p class="p4"><span class="Apple-converted-space">      </span><span class="s1">const</span> bubble = document.getElementById(<span class="s3">'digit-sum'</span>);</p>
<p class="p4"><span class="Apple-converted-space">      </span>bubble.innerText = <span class="s3">`Digit sum: </span><span class="s4">${sum}</span><span class="s3">`</span>;</p>
<p class="p4"><span class="Apple-converted-space">      </span>bubble.style.display = <span class="s3">'block'</span>;</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span><span class="s1">function</span> hideDigitSum() {</p>
<p class="p4"><span class="Apple-converted-space">      </span>document.getElementById(<span class="s3">'digit-sum'</span>).style.display = <span class="s3">'none'</span>;</p>
<p class="p4"><span class="Apple-converted-space">    </span>}</p>
<p class="p6"><br></p>
<p class="p4"><span class="Apple-converted-space">    </span>updateDisplay();</p>
<p class="p3"><span class="s4"><span class="Apple-converted-space">  </span></span>&lt;/script&gt;</p>
<p class="p3">&lt;/body&gt;</p>
<p class="p3">&lt;/html&gt;</p>
